function calculaNota(ex, p1, p2) {
  const prova1= p1*2
  const prova2= p2*3
  const exercicios = ex*1
  const media = (prova1 + prova2 + exercicios)/6
  
  let conceito
  
  if (media >=9){
    conceito = "A"
  }
  else if (media < 9 && media >= 7.5){
    conceito = "B"
  }
  else if (media < 7.5 && media >= 6){
    conceito = "C"
  }
  else {
    conceito = "D"
  }
  return conceito
}